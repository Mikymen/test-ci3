{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.3.3"
      }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ],
    
    "redirects":[
        {
            "source": "/",
            "destination": "/api/"
        }
    ]
  }